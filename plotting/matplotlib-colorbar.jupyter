    def dummy_data(xmin, xmax):
        ymin, ymax = -2, 2
        x, y = np.meshgrid(np.arange(xmin, xmax, 0.1), np.arange(ymin, ymax, 0.1))
        z = (1 - x / 2 + x ** 5 + y ** 3) * np.exp(-x ** 2 - y ** 2)
        extent = x.min(), x.max(), y.min(), y.max()
        return z, extent
