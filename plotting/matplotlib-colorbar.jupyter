     "collapsed": true
    }
code 13
    fig, ax = plt.subplots()
    im = ax.imshow(z, extent=extent, vmin=-zmax, vmax=zmax)
    divider = axes_grid1.make_axes_locatable(ax)
    cax = divider.append_axes('right', size='5%', pad=0.05)
    fig.colorbar(im, cax=cax);
